<h1>View Student's <%= pluralize(@courses.count, "Courses") %></h1>
<table>
	<thead>
		<tr>
			<th>Title</th>
			<th>Professor</th>
			<th>Room</th>
		</tr>
	</thead>
	<tbody>
		<% @courses.each do |course| %>
		<tr>
			<td><%= course.title %></td>
			<td><%= course.professor %></td>
			<td><%= course.room %></td>
				<% end %>
		</tr>
	</tbody>
</table>
<a href="/students">View Students</a>